CREATE TABLE Photo (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  sol INTEGER NOT NULL,
  camera TEXT AS ie.otormaigh.playground.Camera NOT NULL,
  img TEXT NOT NULL,
  earth_date TEXT NOT NULL,
  rover TEXT AS ie.otormaigh.playground.Rover NOT NULL
);

selectAll:
SELECT * FROM Photo;

insert:
INSERT
INTO Photo
VALUES ?;

upsert:
UPDATE Photo
SET sol = ?,
    camera = ?,
    img = ?,
    earth_date = ?,
    rover = ?
WHERE id = ?;